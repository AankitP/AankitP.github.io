{"ast":null,"code":"import TopBar from \"./Desktop_components/TopBar.vue\";\nimport MainBox from \"./Desktop_components/Reusable/MainBox.vue\";\nimport HomePage from \"./Desktop_components/Home.vue\";\nimport ProjectsDone from \"./Desktop_components/Projects.vue\";\nimport ContactPage from \"./Desktop_components/ContactAndSocials.vue\";\nimport OofPage from \"./Desktop_components/404Page.vue\";\nimport AboutMe from \"./Desktop_components/AboutMe.vue\";\nconst [DeviceType, setDeviceType] = useState(getThemeStorage());\nexport default {\n  data: function () {\n    return {\n      accessThis: \"Home\",\n      DeviceType: \"Desktop\"\n    };\n  },\n  name: \"App\",\n  components: {\n    TopBar,\n    MainBox,\n    HomePage,\n    ProjectsDone,\n    ContactPage,\n    OofPage,\n    AboutMe\n  },\n  methods: {\n    getPage(value) {\n      this.accessThis = value;\n    },\n    getDeviceType() {\n      this.DeviceType = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ? \"Mobile\" : \"Desktop\";\n      console.log(this.DeviceType); // \"Mobile\" or \"Desktop\"\n    }\n  }\n};","map":{"version":3,"names":["TopBar","MainBox","HomePage","ProjectsDone","ContactPage","OofPage","AboutMe","DeviceType","setDeviceType","useState","getThemeStorage","data","accessThis","name","components","methods","getPage","value","getDeviceType","test","navigator","userAgent","console","log"],"sources":["/Users/aankitpokhrel/Documents/AankitSiteVue/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"allHolderDesktop\" v-if=\"this.DeviceType == 'Desktop'\">\n    <header>\n      <TopBar class=\"topBar\" @clicked=\"getPage\" />\n    </header>\n    <MainBox>\n      <HomePage v-if=\"accessThis == 'Home'\" />\n      <ProjectsDone v-else-if=\"accessThis == 'Projects'\" />\n      <ContactPage v-else-if=\"accessThis == 'Contact'\" />\n      <AboutMe v-else-if=\"accessThis == 'AboutMe'\" />\n      <OofPage v-else />\n    </MainBox>\n  </div>\n  <div class=\"allHolderMobile\" v-if=\"this.DeviceType == 'Mobile'\">\n    <MainBox>\n      <HomePage v-if=\"accessThis == 'Home'\" />\n      <ProjectsDone v-else-if=\"accessThis == 'Projects'\" />\n      <ContactPage v-else-if=\"accessThis == 'Contact'\" />\n      <AboutMe v-else-if=\"accessThis == 'AboutMe'\" />\n      <OofPage v-else />\n    </MainBox>\n  </div>\n</template>\n<script>\nimport TopBar from \"./Desktop_components/TopBar.vue\";\nimport MainBox from \"./Desktop_components/Reusable/MainBox.vue\";\nimport HomePage from \"./Desktop_components/Home.vue\";\nimport ProjectsDone from \"./Desktop_components/Projects.vue\";\nimport ContactPage from \"./Desktop_components/ContactAndSocials.vue\";\nimport OofPage from \"./Desktop_components/404Page.vue\";\nimport AboutMe from \"./Desktop_components/AboutMe.vue\";\n\nconst [DeviceType, setDeviceType] = useState(getThemeStorage());\n\nexport default {\n  data: function () {\n    return {\n      accessThis: \"Home\",\n      DeviceType: \"Desktop\",\n    };\n  },\n  name: \"App\",\n  components: {\n    TopBar,\n    MainBox,\n    HomePage,\n    ProjectsDone,\n    ContactPage,\n    OofPage,\n    AboutMe,\n  },\n  methods: {\n    getPage(value) {\n      this.accessThis = value;\n    },\n    getDeviceType() {\n      this.DeviceType =\n        /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(\n          navigator.userAgent\n        )\n          ? \"Mobile\"\n          : \"Desktop\";\n      console.log(this.DeviceType); // \"Mobile\" or \"Desktop\"\n    },\n  },\n};\n</script>\n\n<style>\nhtml {\n  background-color: #175588;\n  width: auto;\n  height: auto;\n}\nbody {\n  background-color: #175588;\n}\n.allHolderDesktop {\n  padding: 0 !important;\n  margin: 0 !important;\n  /* min-width: 100rem; */\n  height: auto;\n  background-color: #175588;\n}\n</style>\n"],"mappings":"AAwBA,OAAOA,MAAK,MAAO,iCAAiC;AACpD,OAAOC,OAAM,MAAO,2CAA2C;AAC/D,OAAOC,QAAO,MAAO,+BAA+B;AACpD,OAAOC,YAAW,MAAO,mCAAmC;AAC5D,OAAOC,WAAU,MAAO,4CAA4C;AAC