{"ast":null,"code":"// Import desktop components\nimport TopBar from \"./Desktop_components/TopBar.vue\";\nimport MainBox from \"./Desktop_components/Reusable/MainBox.vue\";\nimport HomePage from \"./Desktop_components/Home.vue\";\nimport ProjectsDone from \"./Desktop_components/Projects.vue\";\nimport ContactPage from \"./Desktop_components/ContactAndSocials.vue\";\nimport OofPage from \"./Desktop_components/404Page.vue\";\nimport AboutMe from \"./Desktop_components/AboutMe.vue\";\n\n// Import mobile components\n// import TopBar_Mobile from \"./Mobile_components/TopBar_Mobile.vue\";\n// import ProjectsDone_Mobile from \"./Mobile_components/Projects_Mobile.vue\";\n\nconst DeviceType = () => /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ? \"Mobile\" : \"Desktop\";\nconsole.log(DeviceType()); // \"Mobile\" or \"Desktop\"\n\nexport default {\n  data: function () {\n    return {\n      accessThis: \"Home\",\n      DeviceType: DeviceType()\n    };\n  },\n  name: \"App\",\n  components: {\n    TopBar,\n    MainBox,\n    HomePage,\n    ProjectsDone,\n    ContactPage,\n    OofPage,\n    AboutMe\n  },\n  methods: {\n    getPage(value) {\n      this.accessThis = value;\n    },\n    getDeviceType() {\n      this.DeviceType = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ? \"Mobile\" : \"Desktop\";\n      console.log(this.DeviceType); // \"Mobile\" or \"Desktop\"\n    }\n  }\n};","map":{"version":3,"names":["TopBar","MainBox","HomePage","ProjectsDone","ContactPage","OofPage","AboutMe","DeviceType","test","navigator","userAgent","console","log","data","accessThis","name","components","methods","getPage","value","getDeviceType"],"sources":["/Users/aankitpokhrel/Documents/AankitSiteVue/src/App.vue"],"sourcesContent":["<template>\n  <header>\n    <TopBar class=\"topBar\" @clicked=\"getPage\" />\n  </header>\n  <div class=\"allHolderDesktop\" v-if=\"DeviceType == 'Desktop'\">\n    <MainBox>\n      <HomePage v-if=\"accessThis == 'Home'\" />\n      <ProjectsDone v-else-if=\"accessThis == 'Projects'\" />\n      <ContactPage v-else-if=\"accessThis == 'Contact'\" />\n      <AboutMe v-else-if=\"accessThis == 'AboutMe'\" />\n      <OofPage v-else />\n    </MainBox>\n  </div>\n  <div class=\"allHolderMobile\" v-if=\"DeviceType == 'Mobile'\">\n    <MainBox>\n      <HomePage v-if=\"accessThis == 'Home'\" />\n      <ProjectsDone v-else-if=\"accessThis == 'Projects'\" />\n      <ContactPage v-else-if=\"accessThis == 'Contact'\" />\n      <AboutMe v-else-if=\"accessThis == 'AboutMe'\" />\n      <OofPage v-else />\n    </MainBox>\n  </div>\n</template>\n\n<script>\n// Import desktop components\nimport TopBar from \"./Desktop_components/TopBar.vue\";\nimport MainBox from \"./Desktop_components/Reusable/MainBox.vue\";\nimport HomePage from \"./Desktop_components/Home.vue\";\nimport ProjectsDone from \"./Desktop_components/Projects.vue\";\nimport ContactPage from \"./Desktop_components/ContactAndSocials.vue\";\nimport OofPage from \"./Desktop_components/404Page.vue\";\nimport AboutMe from \"./Desktop_components/AboutMe.vue\";\n\n// Import mobile components\n// import TopBar_Mobile from \"./Mobile_components/TopBar_Mobile.vue\";\n// import ProjectsDone_Mobile from \"./Mobile_components/Projects_Mobile.vue\";\n\nconst DeviceType = () =>\n  /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(\n    navigator.userAgent\n  )\n    ? \"Mobile\"\n    : \"Desktop\";\nconsole.log(DeviceType()); // \"Mobile\" or \"Desktop\"\n\nexport default {\n  data: function () {\n    return {\n      accessThis: \"Home\",\n      DeviceType: DeviceType(),\n    };\n  },\n  name: \"App\",\n  components: {\n    TopBar,\n    MainBox,\n    HomePage,\n    ProjectsDone,\n    ContactPage,\n    OofPage,\n    AboutMe,\n  },\n  methods: {\n    getPage(value) {\n      this.accessThis = value;\n    },\n    getDeviceType() {\n      this.DeviceType =\n        /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(\n          navigator.userAgent\n        )\n          ? \"Mobile\"\n          : \"Desktop\";\n      console.log(this.DeviceType); // \"Mobile\" or \"Desktop\"\n    },\n  },\n};\n</script>\n\n<style>\nhtml {\n  background-color: #175588;\n  width: auto;\n  height: auto;\n}\nbody {\n  background-color: #175588;\n}\n.allHolderDesktop {\n  padding: 0 !important;\n  margin: 0 !important;\n  /* min-width: 100rem; */\n  height: auto;\n  background-color: #175588;\n}\n</style>\n"],"mappings":"AAyBA;AACA,OAAOA,MAAK,MAAO,iCAAiC;AACpD,OAAOC,OAAM,MAAO,2CAA2C;AAC/D,OAAOC,QAAO,MAAO,+BAA+B;AACpD,OAAOC,YAAW,MAAO,mCAAmC;AAC5D,OAAOC,WAAU,MAAO,4CAA4C;AACpE,OAAOC,OAAM,MAAO,kCAAkC;AACtD,OAAOC,OAAM,MAAO,kCAAkC;;AAEtD;AACA;AACA;;AAEA,MAAMC,UAAS,GAAIA,CAAA,KACjB,gEAAgE,CAACC,IAAI,CACnEC,SAAS,CAACC,SACZ,IACI,QAAO,GACP,SAAS;AACfC,OAAO,CAACC,GAAG,CAACL,UAAU,CAAC,CAAC,CAAC,EAAE;;AAE3B,eAAe;EACbM,IAAI,EAAE,SAAAA,CAAA,EAAY;IAChB,OAAO;MACLC,UAAU,EAAE,MAAM;MAClBP,UAAU,EAAEA,UAAU,CAAC;IACzB,CAAC;EACH,CAAC;EACDQ,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVhB,MAAM;IACNC,OAAO;IACPC,QAAQ;IACRC,YAAY;IACZC,WAAW;IACXC,OAAO;IACPC;EACF,CAAC;EACDW,OAAO,EAAE;IACPC,OAAOA,CAACC,KAAK,EAAE;MACb,IAAI,CAACL,UAAS,GAAIK,KAAK;IACzB,CAAC;IACDC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACb,UAAS,GACZ,gEAAgE,CAACC,IAAI,CACnEC,SAAS,CAACC,SACZ,IACI,QAAO,GACP,SAAS;MACfC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACL,UAAU,CAAC,EAAE;IAChC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}